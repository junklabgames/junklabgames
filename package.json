{
  "name": "junklabgames.com",
  "private": true,
  "description": "Junklabgames.com primary marketing website",
  "version": "0.0.1",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
