    "gatsby-plugin-sharp": "^2.6.43",
    "gatsby-source-filesystem": "^2.3.35",
    "gatsby-transformer-sharp": "^2.5.19",
    "gh-pages": "^3.1.0",
    "node-sass": "^4.14.1",
    "prettier": "2.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/junklabgames/junklabgames"
  },
  "bugs": {
    "url": "https://github.com/junklabgames/junklabgames/issues"
